<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ktdsuniversity.edu.pms.member.dao.PhonePlanDao">

	<select id="selectAllPhonePlans"
		resultType="com.ktdsuniversity.edu.pms.member.vo.PhonePlanVO">
		SELECT PHN_PLAN_ID
		, PHN_PLAN_NAME
		, PHN_PLAN_PRICE
		, MIN_AGE
		, MAX_AGE
		FROM PHONE_PLAN
	</select>

</mapper>